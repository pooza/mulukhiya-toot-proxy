<%# encoding: UTF-8 -%>
SELECT
  token.id
FROM
  oauth_tokens AS token
  INNER JOIN apps ON token.app_id = apps.id
  INNER JOIN users ON token.user_id = users.id
WHERE (users.uri IS null)
  AND (users.is_active = true)
  AND (apps.client_name LIKE '<%= package.name %>%')
GROUP BY
  token.id
ORDER BY
  token.id DESC
